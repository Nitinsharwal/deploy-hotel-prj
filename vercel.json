{
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "hotel_prj/asgi.py" }
  ]
}
